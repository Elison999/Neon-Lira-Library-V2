            PFrame.BackgroundColor3 = Aether.CurrentTheme.Secondary
            PFrame.Parent = TabPage
            Instance.new("UICorner", PFrame).CornerRadius = UDim.new(0, 4)
            
            T.Text = title
            T.Size = UDim2.new(1, -10, 0, 20)
            T.Position = UDim2.new(0, 5, 0, 5)
            T.BackgroundTransparency = 1
            T.TextColor3 = Aether.CurrentTheme.Text
            T.Font = Enum.Font.GothamBold
            T.TextSize = 13
            T.TextXAlignment = Enum.TextXAlignment.Left
            T.Parent = PFrame
            
            D.Text = desc
            D.Size = UDim2.new(1, -10, 0, 30)
            D.Position = UDim2.new(0, 5, 0, 25)
            D.BackgroundTransparency = 1
            D.TextColor3 = Color3.fromRGB(150,150,150)
            D.Font = Enum.Font.Gotham
            D.TextSize = 12
            D.TextWrapped = true
            D.TextXAlignment = Enum.TextXAlignment.Left
            D.Parent = PFrame
        end
        
        -- 12. IMAGE
        function Elements:AddImage(id)
             local Img = Instance.new("ImageLabel")
             Img.Size = UDim2.new(0, 100, 0, 100)
             Img.BackgroundTransparency = 1
             Img.Image = "rbxassetid://" .. id
             Img.Parent = TabPage
        end

        -- 13. COLOR PICKER (Basico RGB)
        function Elements:AddColorPicker(info)
            -- Implementação simplificada para não estourar tokens
            -- Usa sliders vermelhos, verdes e azuis em um dropdown
            Elements:AddSection(info.Name)
            Elements:AddSlider({Name = "R", Max = 255, Callback = function(v) end})
            Elements:AddSlider({Name = "G", Max = 255, Callback = function(v) end})
            Elements:AddSlider({Name = "B", Max = 255, Callback = function(v) end})
        end

        return Elements
    end

    return WindowFunctions
end

return Aether
